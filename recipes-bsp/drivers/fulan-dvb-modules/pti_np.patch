+int pti_task(void *data)
+{
+	TCDMAConfig_t *DMAConfig_p;
+	int vLoopDMAs;
+	U32 WritePtr_physical;
+	U32 ReadPtr_physical;
+	int time_to_wait = msecs_to_jiffies(waitMS);
