+	u16 OddKey7;
+	u16 OddKey6;
+	u16 OddKey5;
+	u16 OddKey4;
+
+	u16 OddIV7;
+	u16 OddIV6;
+	u16 OddIV5;
+	u16 OddIV4;
+	u16 OddIV3;
+	u16 OddIV2;
+	u16 OddIV1;
+	u16 OddIV0;
